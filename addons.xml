<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.990"
       name="990.ro"
       version="0.5.10"
       provider-name="krysty">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py"><provides>video</provides></extension>
	<extension point="xbmc.service" library="service.py" start="startup"></extension>
    <extension point="xbmc.addon.metadata">
		<provides>video</provides>
        <platform>all</platform>
		<website>https://github.com/yokrysty/krysty-xbmc</website>
        <license>GNU GENERAL PUBLIC LICENSE. Version 3, 29 June 2007</license>
        <summary>Stream videos from www.990.ro</summary>
        <description>Stream scene releases from the links listed on [COLOR blue]www.990.ro[/COLOR]</description>
    </extension>
</addon>

<addon
    id="plugin.video.livetvonline"
    name="LiveTvOnline.ro"
    version="2.0.5"
    provider-name="krysty">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py"><provides>video</provides></extension>
    <extension point="xbmc.addon.metadata">
        <provides>video</provides>
        <platform>all</platform>
		<website>https://github.com/yokrysty/krysty-xbmc</website>
        <license>GNU GENERAL PUBLIC LICENSE. Version 3, 29 June 2007</license>
        <summary>Televiziune Online</summary>
        <description>Vizionare programe TV listate pe [COLOR blue]https://www.livetvonline.ro/[/COLOR]. Aceasta aplicatie este independenta de site-ul livetvonline.ro si este oferita gratuit. Pentru a putea viziona toate programele TV trebuie sa detineti un cont cu abonament pe site-ul livetvonline.ro</description>
    </extension>
</addon>

<addon id="repository.krysty-xbmc" name="Krysty's Addons" version="1.0.0" provider-name="krysty">
	<extension point="xbmc.addon.repository" name="Krysty's Addons">
		<info compressed="false">https://raw.githubusercontent.com/yokrysty/krysty-xbmc/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/yokrysty/krysty-xbmc/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/yokrysty/krysty-xbmc/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Addons from https://github.com/yokrysty/krysty-xbmc</summary>
		<description>Download and install addons for Kodi (formerly known as XBMC).</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="service.subtitles.opensubtitles"
       name="OpenSubtitles.org"
       version="5.0.14"
       provider-name="amet">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.subtitle.module"
             library="service.py" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">OpenSubtitles</summary>
    <description lang="en">Search and download subtitles from opensubtitles.org</description>
    <description lang="zh">从 opensubtitles.org 搜索并下载字幕</description>
    <disclaimer lang="en"></disclaimer>
    <platform>all</platform>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum>http://forum.xbmc.org/showthread.php?tid=183114</forum>
    <website></website>
    <email></email>
    <source>https://github.com/amet/service.subtitles.opensubtitles</source>
  </extension>
</addon>
</addons>
